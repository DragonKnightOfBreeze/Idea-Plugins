{
  "definitions" : {
    "fallen_empire" : {
      "type" : "object",
      "properties" : {
        "graphical_culture" : {
          "$type" : "graphical_culture"
        },
        "initializer" : {
          "$type" : "initializer"
        },
        "weight_modifier" : {
          "$ref" : "#/definitions/weight_modifier"
        },
        "create_country_effect" : {
          "$ref" : "#/definitions/create_country_effect"
        }
      }
    },
    "create_country_effect" : {
      "type" : "object",
      "properties" : {
        "create_species" : {
          "type" : "object",
          "properties" : {
            "name" : {
              "anyOf" : [ {
                "type" : "string"
              }, {
                "const" : "random"
              } ]
            },
            "class" : {
              "$type" : "species_class"
            },
            "portrait" : {
              "anyOf" : [ {
                "$type" : "portrait"
              }, {
                "const" : "random"
              } ]
            },
            "traits" : {
              "anyOf" : [ {
                "$type" : "trait"
              }, {
                "const" : "random"
              } ]
            },
            "extra_trait_points" : {
              "type" : "integer"
            },
            "allow_negative_traits" : {
              "type" : "boolean"
            }
          }
        },
        "create_country" : {
          "type" : "object",
          "properties" : {
            "name" : {
              "anyOf" : [ {
                "type" : "string"
              }, {
                "const" : "random"
              } ]
            },
            "type" : {
              "$type" : "country_type"
            },
            "ignore_initial_colony_error" : {
              "type" : "boolean"
            },
            "authority" : {
              "$type" : "authority"
            },
            "civics" : {
              "type" : "object",
              "properties" : {
                "civic" : {
                  "$type" : "civic",
                  "multiple" : true
                }
              }
            },
            "species" : {
              "$type" : "species"
            },
            "ethos" : {
              "type" : "object",
              "properties" : {
                "ethic" : {
                  "$type" : "ethic",
                  "multiple" : true
                }
              }
            },
            "flag" : {
              "anyOf" : [ {
                "$type" : "flag"
              }, {
                "const" : "random"
              } ]
            },
            "origin" : {
              "$type" : "origin"
            },
            "effect" : {
              "$ref" : "#/definitions/effects"
            }
          }
        }
      }
    },
    "effects" : {
      "type" : "object",
      "anyOf" : [ {
        "$ref" : "#/definitions/condition_expressions"
      }, {
        "$ref" : "#/definitions/boolean_expressions"
      }, {
        "$ref" : "#/definitions/scope_expressions"
      }, {
        "$ref" : "#/definitions/scripted_triggers"
      } ]
    },
    "weight_modifier" : {
      "type" : "object",
      "properties" : {
        "base" : {
          "type" : "integer"
        },
        "modifier" : {
          "allOf" : [ {
            "anyOf" : [ {
              "properties" : {
                "factor" : {
                  "type" : "number"
                }
              }
            }, {
              "properties" : {
                "add" : {
                  "type" : "number"
                }
              }
            } ]
          }, {
            "$ref" : "#/definitions/modifier"
          } ],
          "multiple" : true
        }
      }
    },
    "condition_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/condition_expression_name"
      },
      "properties" : {
        "limit" : {
          "$ref" : "#/definitions/triggers"
        }
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "boolean_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/boolean_expression_name"
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "scope_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/scope_name"
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "scripted_triggers" : {
      "type" : "object",
      "additionalProperties" : true,
      "$type" : "scripted_trigger"
    },
    "modifier" : {
      "type" : "object",
      "additionalProperties" : true
    },
    "condition_expression_name" : {
      "type" : "string",
      "enum" : [ "AND", "OR", "NOT", "NAND", "NOR" ]
    },
    "boolean_expression_name" : {
      "type" : "string",
      "enum" : [ "if", "else", "else_if", "AND", "OR", "NOT", "NAND", "NOR" ]
    },
    "scope_name" : {
      "type" : "string",
      "enum" : [ "capital", "controller", "root", "from", "owner", "planet", "prev", "random", "this", "pop" ]
    },
    "triggers" : {
      "type" : "object",
      "anyOf" : [ {
        "$ref" : "#/definitions/condition_expressions"
      }, {
        "$ref" : "#/definitions/boolean_expressions"
      }, {
        "$ref" : "#/definitions/scope_expressions"
      }, {
        "$ref" : "#/definitions/scripted_triggers"
      } ]
    }
  },
  "type" : "object",
  "additionalProperties" : {
    "$ref" : "#/definitions/fallen_empire"
  }
}