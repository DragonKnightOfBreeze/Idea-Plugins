{
  "definitions" : {
    "pop_faction_type" : {
      "type" : "object",
      "properties" : {
        "election_header" : {
          "$ref" : "#/definitions/gfx_reference"
        },
        "guiding_ethic" : {
          "$type" : "ethic"
        },
        "unique" : {
          "type" : "boolean"
        },
        "support_multiplier" : {
          "type" : "integer"
        },
        "resources" : {
          "$ref" : "#/definitions/economy_unit"
        },
        "is_potential" : {
          "$ref" : "#/definitions/triggers"
        },
        "parameters" : {
          "$ref" : "#/definitions/parameters"
        },
        "can_join_pre_triggers" : {
          "$ref" : "#/definitions/triggers"
        },
        "can_join_faction" : {
          "$ref" : "#/definitions/triggers"
        },
        "attraction" : {
          "$ref" : "#/definitions/weight_modifier"
        },
        "leader" : {
          "$ref" : "#/definitions/weight_modifier"
        },
        "demand" : {
          "$ref" : "#/definitions/demand"
        }
      }
    },
    "parameters" : {
      "type" : "object",
      "properties" : {
        "empire" : {
          "type" : "object",
          "properties" : {
            "type" : {
              "type" : "string"
            },
            "valid_objects" : {
              "$ref" : "#/definitions/triggers"
            }
          }
        }
      }
    },
    "demand" : {
      "type" : "object",
      "properties" : {
        "title" : {
          "$ref" : "#/definitions/loc_reference"
        },
        "unfulfilled_title" : {
          "$ref" : "#/definitions/loc_reference"
        },
        "desc" : {
          "$ref" : "#/definitions/loc_reference"
        },
        "fulfilled_effect" : {
          "type" : "number"
        },
        "unfulfilled_effect" : {
          "type" : "number"
        },
        "potential" : {
          "$ref" : "#/definitions/triggers"
        },
        "trigger" : {
          "$ref" : "#/definitions/triggers"
        }
      }
    },
    "economy_unit" : {
      "type" : "object",
      "properties" : {
        "category" : {
          "$type" : "economic_category"
        },
        "produces" : {
          "$ref" : "#/definitions/resources_item"
        },
        "cost" : {
          "$ref" : "#/definitions/resources_item"
        },
        "upkeep" : {
          "$ref" : "#/definitions/resources_item"
        }
      }
    },
    "triggers" : {
      "type" : "object",
      "anyOf" : [ {
        "$ref" : "#/definitions/condition_expressions"
      }, {
        "$ref" : "#/definitions/boolean_expressions"
      }, {
        "$ref" : "#/definitions/scope_expressions"
      }, {
        "$ref" : "#/definitions/scripted_triggers"
      } ]
    },
    "weight_modifier" : {
      "type" : "object",
      "properties" : {
        "base" : {
          "type" : "number"
        },
        "modifier" : {
          "$ref" : "#/definitions/modifier",
          "multiple" : true
        }
      }
    },
    "loc_reference" : {
      "type" : "string",
      "anyOf" : [ {
        "$def" : "localisation/**/*.yml/#/{property}"
      }, {
        "$def" : "localisation_synced/**/*.yml/#/{property}"
      } ]
    },
    "gfx_reference" : {
      "type" : "string",
      "$def" : "interface/**/*.gfx/#/{spliteType}/name"
    },
    "resources_item" : {
      "type" : "object",
      "anyOf" : [ {
        "properties" : {
          "trigger" : {
            "$ref" : "#/definitions/triggers"
          }
        }
      }, {
        "propertyNames" : {
          "$type" : "resource"
        },
        "additionalProperties" : {
          "type" : "integer"
        }
      } ]
    },
    "condition_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/condition_expression_name"
      },
      "properties" : {
        "limit" : {
          "$ref" : "#/definitions/triggers"
        }
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "boolean_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/boolean_expression_name"
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "scope_expressions" : {
      "type" : "object",
      "propertyNames" : {
        "$ref" : "#/definitions/scope_name"
      },
      "additionalProperties" : {
        "$ref" : "#/definitions/triggers"
      }
    },
    "scripted_triggers" : {
      "type" : "object",
      "additionalProperties" : true,
      "$type" : "scripted_trigger"
    },
    "modifier" : {
      "type" : "object"
    },
    "condition_expression_name" : {
      "type" : "string",
      "enum" : [ "AND", "OR", "NOT", "NAND", "NOR" ]
    },
    "boolean_expression_name" : {
      "type" : "string",
      "enum" : [ "if", "else", "else_if", "AND", "OR", "NOT", "NAND", "NOR" ]
    },
    "scope_name" : {
      "type" : "string",
      "enum" : [ "capital", "controller", "root", "from", "owner", "planet", "prev", "random", "this", "pop" ]
    }
  },
  "type" : "object",
  "additionalProperties" : {
    "$ref" : "#/definitions/pop_faction_type"
  }
}